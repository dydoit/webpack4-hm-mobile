/*global style*/
html
  overflow-x hidden
  height 100%
  -webkit-tap-highlight-color transparent
body
  font:13px PingFangSC Helvetica, Arial, sans-serif
.clearfix
  &:after
  &:before
    content:'';
    display:table;
  &:after
    clear both
.auto
  margin-left:auto;
  margin-right:auto;
.mt10
  margin-top 10px
