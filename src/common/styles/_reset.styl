/*css reset*/
body,dl,dd,ul,ol,h1,h2,h3,h4,h5,h6,p,form,input
  margin:0;
ul,ol
  padding:0;
  list-style:none;
h1,h2,h3,h4,h5,h6,b,strong
  font-weight:normal;
audio,canvas,video
  display:inline-block;
input,textarea,button,select,a
  outline:0 none;
  border: none;
img
  border:0;
  vertical-align:middle;
a
  text-decoration none
button 
  padding 0
